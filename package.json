{
  "name": "storybook-addon-deps",
  "version": "2.0.0",
  "description": "A storybook addon to display component dependencies",
  "keywords": [
    "addon",
    "dependencies",
    "webpack",
    "storybook"
  ],
  "homepage": "https://github.com/atanasster/storybook-addon-deps/",
  "bugs": "https://github.com/atanasster/storybook-addon-deps/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/atanasster/storybook-addon-deps.git"
  },
  "license": "MIT",
  "author": "Atanas Stoyanov",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "start-storybook -p 9001",
    "debug": "NODE_OPTIONS=--inspect-brk  start-storybook -p 9001",
    "build": "rollup -c && tsc",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "pub": "pushd ./ && npm run build && cd dist && npm publish && popd"
  },
  "peerDependencies": {
    "@storybook/addons": "*",
    "@storybook/addon-docs": "*",
    "@storybook/api": "*",
    "@storybook/client-api": "*",
    "@storybook/components": "*",
    "@storybook/react": "*",
    "@storybook/router": "*",
    "@storybook/theming": "*",
    "react": ">= 16.8.3",
    "react-dom": ">= 16.8.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "next",
    "@storybook/addon-docs": "next",
    "@storybook/addons": "next",
    "@storybook/api": "next",
    "@storybook/client-api": "next",
    "@storybook/components": "next",
    "@storybook/design-system": "^2.0.5",
    "@storybook/react": "next",
    "@storybook/router": "next",
    "@storybook/theming": "next",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.2",
    "eslint": "^6.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-copy-glob": "^0.3.1",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.22.1",
    "storybook-dark-mode": "^0.1.9",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "memoizerific": "^1.11.3",
    "react-dnd": "8.0.3",
    "react-google-charts": "^3.0.15",
    "react-sortable-tree": "^2.6.2",
    "storybook-dep-webpack-plugin": "^1.0.3"
  }
}
